SET SERVEROUTPUT ON
CREATE OR REPLACE PROCEDURE PRINT_DAY(DATE_INPUT DATE)
AS
DAYNAME VARCHAR2(10);
BEGIN
   DAYNAME:=TO_CHAR(DATE_INPUT,'DAY');
   DBMS_OUTPUT.PUT_LINE(DAYNAME);
END;
/
EXEC PRINT_DAY(SYSDATE);


SET SERVEROUTPUT ON
CREATE OR REPLACE PROCEDURE GET_DAY
AS
DAYNAME VARCHAR2(10);
BEGIN

   DAYNAME:=TO_CHAR(CURRENT_DATE(),'DAY');
   DBMS_OUTPUT.PUT_LINE(DAYNAME);
END;
/

SET SERVEROUTPUT ON
CREATE OR REPLACE PROCEDURE staj_utility
AS
BEGIN
PRINT_DAY(SYSDATE);
GET_DAY;
END;
/

EXEC staj_utility;

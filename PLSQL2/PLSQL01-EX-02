DECLARE
FIRST_NAME_1 VARCHAR(10):= INITCAP(LOWER('ERMAN'));
LAST_NAME_1 VARCHAR(10):= INITCAP(LOWER('İbriŞiM'));
FIRST_NAME_2 VARCHAR(10):=(LOWER('MUstAFA'));
LAST_NAME_2 VARCHAR(10):=(LOWER('Demir'));
BEGIN
DBMS_OUTPUT.PUT_LINE('Name - Surname = '|| FIRST_NAME_1||' '||LAST_NAME_1 );
DBMS_OUTPUT.PUT_LINE('Name - Surname = '|| FIRST_NAME_2||' '||LAST_NAME_2 );
END;
/

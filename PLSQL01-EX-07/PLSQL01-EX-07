SELECT * FROM HR.EMPLOYEES;
SELECT * FROM HR.DEPARTMENTS;

SELECT 
    D.DEPARTMENT_ID, E.FIRST_NAME, E.SALARY, 
DENSE_RANK() 
OVER (
    PARTITION BY 
        D.DEPARTMENT_ID 
        ORDER BY 
            E.SALARY) 
AS RANK
FROM 
    HR.EMPLOYEES E, HR.DEPARTMENTS D
WHERE 
    E.DEPARTMENT_ID = D.DEPARTMENT_ID


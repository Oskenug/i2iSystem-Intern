package controller;

public class exercise {

		public static void main(String[] args) 
		{
			for(int i=2;i<20;i++)
			{
				if((FonkPrimeNumber(i)))
				{
					System.out.println(i);
				}
			}
		}		
		    public static  boolean  FonkPrimeNumber(int PrimeNumber)
		    {
		        boolean count=true;
		        for(int j=2; j <= Math.sqrt(PrimeNumber); j++)
		        {
		            if(PrimeNumber%j==0)
		            {
		                count=false;
		                break;
		            }
		        }		        
		        return count;
		    }
		    }
